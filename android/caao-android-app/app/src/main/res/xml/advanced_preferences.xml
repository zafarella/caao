<?resources.xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
                  android:key="advanced_prefs"
                  android:title="@string/advanced_prefs_title">
    <PreferenceCategory
            android:key="server_info"
            android:title="@string/advanced_prefs_server_into_title">
        <ListPreference
                android:enabled="false"
                android:key="server_info"
                android:summary="@string/advanced_prefs_server_info_summary"
                android:title="@string/advanced_prefs_server_info_title"></ListPreference>
        <EditTextPreference
                android:defaultValue="http://10.0.2.2:8888"
                android:key="server_url"
                android:summary="@string/advanced_prefs_server_url_summary"
                android:title="@string/advanced_prefs_server_url"></EditTextPreference>
    </PreferenceCategory>
</PreferenceScreen>
